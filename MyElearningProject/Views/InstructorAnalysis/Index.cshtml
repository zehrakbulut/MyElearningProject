
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="row">
    <!-- Column -->
    @Html.Action("InstructorPanelPartial","InstructorAnalysis")
    <div class="col-lg-8 col-xlg-9 col-md-7">
        <div class="card">
